{
  "version": 2,
  "buildCommand": "pnpm vercel-build",
  "installCommand": "pnpm install",
  "functions": {
    "api/server.js": {
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": "dist/**"
    }
  },
  "routes": [{ "src": "/(.*)", "dest": "/api/server.js" }],
  "outputDirectory": "public",
  "framework": null,
  "env": {
    "NODE_ENV": "production"
  }
}
